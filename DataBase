CREATE DATABASE db_inmobiliaria_final;

USE db_inmobiliaria_final;

CREATE TABLE subtipo_propiedad(
	codigo_subtipopropiedada VARCHAR (20) NOT NULL PRIMARY KEY,
	departamento VARCHAR (10) NOT NULL,
	casa VARCHAR (10) NOT NULL,
	local_ VARCHAR (10) NOT NULL,
	oficina VARCHAR (10) NOT NULL
);

DESCRIBE subtipo_propiedad;

CREATE TABLE distribucion_propiedad(
	codigo_distribucion_propiedad VARCHAR (20) NOT NULL PRIMARY KEY,
	habitaciones INTEGER (2) NOT NULL,
	banhios  INTEGER (3) NOT NULL,
	balcon INTEGER (1) NOT NULL,
	cuarto_util BOOL NOT NULL
);

DESCRIBE distribucion_propiedad;


CREATE TABLE disponibilidad_propiedad(
	codigo_disponibilidad_propiedad VARCHAR (20) NOT NULL PRIMARY KEY,
	venta BOOL NOT NULL,
	alquiler BOOL NOT NULL,
	ambas BOOL NOT NULL 
);

DESCRIBE disponibilidad_propiedad;


CREATE TABLE tipo_propietario(
	codigo_tipo_propietario VARCHAR (20) NOT NULL PRIMARY KEY,
	persona_fisica BOOL NOT NULL,
	juridica BOOL NOT NULL 
);

DESCRIBE tipo_propietario;


CREATE TABLE documento_identidad(
	codigo_documento_identidad VARCHAR (20) NOT NULL PRIMARY KEY,
	registro_civil INTEGER (2) NOT NULL,
	tarjeta_identidad INTEGER (2) NOT NULL,
	cedula_ciudadania INTEGER (2) NOT NULL,
	pasaporte INTEGER (2) NOT NULL,
	cedula_extranjeria INTEGER (2) NOT NULL,
	rut INTEGER (2) NOT NULL
);

DESCRIBE documento_identidad;


CREATE TABLE visita_propiedades (
   codigo_visita_propiedad VARCHAR(20) NOT NULL PRIMARY KEY,
   codigo_propiedad INT NOT NULL,
   fecha_visita DATE NOT NULL,
   hora_visita TIME NOT NULL,
   nombre_cliente VARCHAR(100) NOT NULL,
   nombre_agente VARCHAR(100) NOT NULL,
   duracion_minutos INT NOT NULL,
   interes_mostrado VARCHAR(50),
   observaciones_cliente VARCHAR(255),
   proximos_pasos_acordados VARCHAR(255),
   seguimiento_posterior VARCHAR(255)
);

DESCRIBE visita_propiedades;

CREATE TABLE operaciones_venta(
	codigo_operaciones_venta VARCHAR (20) NOT NULL PRIMARY KEY,
	numero_referencia INTEGER (31) NOT NULL,
	fecha DATE NOT NULL,
	propiedad VARCHAR (28) NOT NULL,
	vendedor VARCHAR (23) NOT NULL,
	comprador VARCHAR (16) NOT NULL,
	precio_final_acordado FLOAT (20) NOT NULL,
	forma_pago VARCHAR (27) NOT NULL,
	reserva_recibida VARCHAR (12) NOT NULL,
	condiciones_especiales VARCHAR (21) NOT NULL,
	fecha_estimada_escritura DATE NOT NULL,
	comisiones_generadas VARCHAR (16) NOT NULL,
	estado_actual VARCHAR (9) NOT NULL,
	documentacion_asociada VARCHAR (8) NOT NULL
);

DESCRIBE operaciones_venta;


CREATE TABLE contratos_alquiler (
   codigo_contrato VARCHAR(20) NOT NULL PRIMARY KEY,
   numero_contrato INT NOT NULL,
   codigo_propiedad INT NOT NULL,
   propiedad VARCHAR(100) NOT NULL,
   nombre_propietario VARCHAR(100) NOT NULL,
   nombre_inquilino VARCHAR(100) NOT NULL,
   duracion_meses INT NOT NULL,
   fecha_inicio DATE NOT NULL,
   fecha_fin DATE NOT NULL,
   renta_mensual_usd FLOAT NOT NULL,
   dia_pago VARCHAR(20) NOT NULL,
   forma_pago VARCHAR(50) NOT NULL,
   deposito_garantia_usd FLOAT,
   servicios_incluidos VARCHAR(255),
   clausulas_especiales VARCHAR(255),
   seguros_asociados VARCHAR(255),
   inventario_propiedad VARCHAR(255),
   estado_contrato VARCHAR(30) NOT NULL,
   fecha_renovacion DATE,
   observaciones VARCHAR(255)
);


DESCRIBE contratos_alquiler;


CREATE TABLE canales_utilizados (
   codigo_canal VARCHAR(20) NOT NULL PRIMARY KEY,
   nombre_canal VARCHAR(100) NOT NULL,
   tipo_canal VARCHAR(50) NOT NULL,
   descripcion VARCHAR(255),
   costo_promedio DECIMAL(10,2),
   eficacia_porcentaje DECIMAL(5,2),
   responsable VARCHAR(100),
   observaciones VARCHAR(255)
);


DESCRIBE canales_utilizados;

CREATE TABLE tipo_inspeccion(
	codigo_tipo_inspeccion VARCHAR (20) NOT NULL PRIMARY KEY,
	general_ VARCHAR (5) NOT NULL,
	especifica VARCHAR (3) NOT NULL 
);

DESCRIBE tipo_inspeccion;

CREATE TABLE tipo_documentos_inmobiliarios(
	codigo_tipo_documentos_inmobiliarios VARCHAR (20) NOT NULL PRIMARY KEY,
	escritura VARCHAR (5) NOT NULL,
	contrato VARCHAR (8) NOT NULL,
	certificado VARCHAR (8) NOT NULL,
	plano VARCHAR (2) NOT NULL 
);

DESCRIBE tipo_documentos_inmobiliarios;

DESCRIBE subtipo_propiedad;
DESCRIBE distribucion_propiedad;
DESCRIBE disponibilidad_propiedad;
DESCRIBE tipo_propietario;
DESCRIBE documento_identidad;
DESCRIBE visita_propiedades;
DESCRIBE operaciones_venta;
DESCRIBE contratos_alquiler;
DESCRIBE canales_utilizados;
DESCRIBE tipo_inspeccion;
DESCRIBE tipo_documentos_inmobiliarios;

CREATE TABLE tipo_propiedad(
	codigo_tipo_propiedad VARCHAR (20) NOT NULL PRIMARY KEY,
	vivienda VARCHAR (5) NOT NULL,
	comercial VARCHAR (6) NOT NULL,
	terreno VARCHAR (2) NOT NULL,
	industrial VARCHAR (4) NOT NULL,
	id_subtipo_propiedad VARCHAR (20),
	FOREIGN KEY (id_subtipo_propiedad) REFERENCES subtipo_propiedad(codigo_subtipopropiedada)
);

DESCRIBE tipo_propiedad;

CREATE TABLE documentos_inmobiliarios(
	codigo_documentos_inmobiliarios VARCHAR (20) NOT NULL PRIMARY KEY,
	codigo INTEGER (16) NOT NULL,
	propiedad_relacionada VARCHAR (7) NOT NULL,
	fecha_emision DATE NOT NULL,
	entidad_emisora VARCHAR (7) NOT NULL,
	estado_vigencia VARCHAR (16) NOT NULL,
	ubicacion_fisica_original VARCHAR (17) NOT NULL,
	version_digitalizada VARCHAR (10) NOT NULL,
	observaciones_legales VARCHAR (15) NOT NULL,
	id_tipo_documento_inmobiliario VARCHAR (8),
	FOREIGN KEY (id_tipo_documento_inmobiliario) REFERENCES tipo_documentos_inmobiliarios(codigo_tipo_documentos_inmobiliarios)	
);

DESCRIBE documentos_inmobiliarios;


CREATE TABLE clientes_potenciales(
	codigo_clientes_potenciales VARCHAR (20) NOT NULL PRIMARY KEY,
	codigo INTEGER NOT NULL,
	nombre VARCHAR (50) NOT NULL,
	telefono INTEGER NOT NULL,
	correo VARCHAR (30) NOT NULL,
	presupuesto_maximo INTEGER NOT NULL,
	zona_interes VARCHAR (30) NOT NULL,
	tipo_propiedad_buscada VARCHAR (30) NOT NULL,
	caracteristicas_deseadas VARCHAR (100) NOT NULL,
	urgencia_busqueda VARCHAR (30) NOT NULL,
	origen_contacto VARCHAR (30) NOT NULL,
	agente_asignado VARCHAR (50) NOT NULL,
	seguimiento_realizado VARCHAR (20) NOT NULL,
	id_tipo_propiedad VARCHAR (20),
	id_subtipo_propiedad VARCHAR (20),
	id_visitas_propiedades VARCHAR (20),
	id_operaciones_venta VARCHAR (20),
	FOREIGN KEY (id_tipo_propiedad) REFERENCES tipo_propiedad(codigo_tipo_propietario),
	FOREIGN KEY (id_subtipo_propiedad) REFERENCES subtipo_propiedad(codigo_subtipopropiedada),
	FOREIGN KEY (id_visitas_propiedades) REFERENCES visita_propiedades(codigo_visita_propiedades),
	FOREIGN KEY (id_operaciones_venta) REFERENCES operaciones_venta(codigo_operaciones_venta)
);

DESCRIBE clientes_potenciales;


CREATE TABLE propiedad(
	codigo_propiedad VARCHAR (20) NOT NULL PRIMARY KEY,
	precio INTEGER NOT NULL,
	codigo_unico INTEGER (10) NOT NULL,
	direccion_completa VARCHAR (9) NOT NULL,
	coordenadas_geograficas INTEGER (5)  NOT NULL,
	superficie_terreno INTEGER  NOT NULL,
	superficie_construida INTEGER  NOT NULL,
	anio_construccion DATE NOT NULL,
	caracteristicas_destacadas VARCHAR (20) NOT NULL,
	estado_conservacion VARCHAR (17) NOT NULL,
	situacion_legal VARCHAR (7) NOT NULL,
	precio_solicitado INTEGER  NOT NULL,
	comision_aplicable INTEGER  NOT NULL,
	propietario VARCHAR (16) NOT NULL,
	id_clientes_potenciales VARCHAR (8),
	id_operaciones_venta VARCHAR (12),
	id_distribucion INTEGER (3),
	id_disponibilidad VARCHAR(5),
	id_tipo_propiedad VARCHAR (5),
	id_subtipo_propiedad VARCHAR (2),
	id_visita_propiedades VARCHAR (9),
	id_contratos_alquiler VARCHAR (14),
	id_documentos_inmobiliarios INTEGER (16),
	FOREIGN KEY (id_clientes_potenciales) REFERENCES clientes_potenciales(codigo_clientes_potenciales),
	FOREIGN KEY (id_operaciones_venta) REFERENCES operaciones_venta(codigo_operaciones_venta),
	FOREIGN KEY (id_distribucion) REFERENCES distribucion_propiedad(codigo_distribucion_propiedad),
	FOREIGN KEY (id_disponibilidad) REFERENCES disponibilidad_propiedad(codigo_disponibilidad_propiedad),
	FOREIGN KEY (id_tipo_propiedad) REFERENCES tipo_propiedad(codigo_tipo_propiedad),
	FOREIGN KEY (id_subtipo_propiedad) REFERENCES subtipo_propiedad(codigo_subtipopropiedada),
	FOREIGN KEY (id_visita_propiedades) REFERENCES visita_propiedades(codigo_visita_propiedades),
	FOREIGN KEY (id_contratos_alquiler) REFERENCES contratos_alquiler(codigo_contratos_alquiler),
	FOREIGN KEY (id_documentos_inmobiliarios) REFERENCES documentos_inmobiliarios(codigo_documentos_inmobiliarios)
);

DESCRIBE propiedad;


CREATE TABLE propietario(
	codigo_propietario VARCHAR (20) NOT NULL  PRIMARY KEY,
	codigo_unico VARCHAR (30) NOT NULL,
	nombre VARCHAR (40) NOT NULL,
	direccion VARCHAR (20) NOT NULL,
	correo_electronico VARCHAR (30) NOT NULL,
	propiedades_registradas INTEGER (5) NOT NULL,
	datos_bancarios_pagos VARCHAR (15) NOT NULL,
	forma_contacto_preferida VARCHAR (16) NOT NULL,
	observaciones_particulares VARCHAR (12) NOT NULL,
	id_propiedad VARCHAR (6),
	id_tipo_propietario VARCHAR (7),
	id_documento_identidad INTEGER (2),
	id_contratos_alquiler VARCHAR (14),
	FOREIGN KEY (id_propiedad) REFERENCES propiedad(codigo_propiedad),
	FOREIGN KEY (id_tipo_propietario) REFERENCES tipo_propietario(codigo_tipo_propietario),
	FOREIGN KEY (id_documento_identidad) REFERENCES documento_identidad(codigo_documento_identidad),
	FOREIGN KEY (id_contratos_alquiler) REFERENCES contratos_alquiler(codigo_contratos_alquiler)
);

DESCRIBE propietario;


CREATE TABLE inspecciones_tecnicas(
	codigo_inspecciones_tecnicas VARCHAR (20) NOT NULL PRIMARY KEY,
	numero_informe INTEGER (25) NOT NULL,
	fecha DATE NOT NULL,
	tecnico_responsable VARCHAR (20) NOT NULL,
	elementos_revisados VARCHAR (18) NOT NULL,
	hallazgos VARCHAR (30) NOT NULL,
	recomendaciones VARCHAR (28) NOT NULL,
	presupuesto_estimado_reparaciones INT NOT NULL,
	documentacion_grafica VARCHAR (14) NOT NULL,
	conclusion_general VARCHAR (38) NOT NULL,
	id_tipo_inspeccion VARCHAR (8),
	id_propiedad VARCHAR (16),
	FOREIGN KEY (id_tipo_inspeccion) REFERENCES tipo_inspeccion(codigo_tipo_inspeccion),
	FOREIGN KEY (id_propiedad) REFERENCES propiedad(codigo_propiedad)	 
);

DESCRIBE inspecciones_tecnicas;

CREATE TABLE agentes_inmobiliarios(
	codigo_agentes_inmobiliarios VARCHAR (20) NOT NULL PRIMARY KEY,
	codigo_empleado INTEGER (25) NOT NULL,
	nombres VARCHAR (20) NOT NULL,
	apellidos VARCHAR (20) NOT NULL,
	fecha_nacimiento DATE NOT NULL,
	direccion VARCHAR (26) NOT NULL, 
	telefono VARCHAR (13) NOT NULL,
	correo_electronico VARCHAR (35) NOT NULL, 
	licencia_profesional INTEGER (20) NOT NULL,
	especializacion_por_zona VARCHAR (5) NOT NULL, 
	comision_pactada FLOAT (4) NOT NULL, 
	ventas_alquileres_concretados INTEGER (3) NOT NULL, 
	evaluacion_desempenio VARCHAR (25) NOT NULL,
	id_operaciones_venta INTEGER (8),
	id_clientes_potenciales VARCHAR (8),
	id_visitas_propiedades VARCHAR (9),
	id_documento_identidad INTEGER (2),
	id_propiedad VARCHAR (6),
	FOREIGN KEY (id_operaciones_venta) REFERENCES operaciones_venta(codigo_operaciones_venta),
	FOREIGN KEY (id_clientes_potenciales) REFERENCES clientes_potenciales(codigo_clientes_potenciales),
	FOREIGN KEY (id_visitas_propiedades) REFERENCES visita_propiedades(codigo_visita_propiedades),
	FOREIGN KEY (id_documento_identidad) REFERENCES documento_identidad(codigo_documento_identidad),
	FOREIGN KEY (id_propiedad) REFERENCES propiedad(codigo_propiedad)
);

DESCRIBE agentes_inmobiliarios;

CREATE TABLE marketing_propiedades(
	codigo_marketing_propiedades VARCHAR (20) NOT NULL PRIMARY KEY,
	codigo_campanhia VARCHAR (20) NOT NULL,
	descripcion_promocional VARCHAR (30) NOT NULL,
	caracteristicas_destacadas VARCHAR (25) NOT NULL,
	fotografias VARCHAR (40) NOT NULL,
	videos VARCHAR (36) NOT NULL,
	planos VARCHAR (30) NOT NULL,
	duracion_campanhia VARCHAR (25) NOT NULL,
	inversion_realizada FLOAT (15) NOT NULL,
	resultados_obtenidos VARCHAR (10) NOT NULL,
	id_canales_utilizados VARCHAR (5),
	id_propiedad VARCHAR (16),
	FOREIGN KEY (id_canales_utilizados) REFERENCES canales_utilizados(codigo_canales_utilizados),
	FOREIGN KEY (id_propiedad) REFERENCES propiedad(codigo_propiedad)
);

DESCRIBE marketing_propiedades;

CREATE TABLE estadisticas_ventas (
    zona VARCHAR(50),
    agente VARCHAR(50),
    total_ventas INT DEFAULT 0,
    PRIMARY KEY (zona, agente)
);

CREATE TABLE historial_precios (
    id_historial INT AUTO_INCREMENT PRIMARY KEY,
    codigo_propiedad VARCHAR(20),
    precio_anterior FLOAT,
    nuevo_precio FLOAT,
    fecha_cambio DATETIME
);


#Tablas completas

DESCRIBE subtipo_propiedad;
DESCRIBE distribucion_propiedad;
DESCRIBE disponibilidad_propiedad;
DESCRIBE tipo_propietario;
DESCRIBE documento_identidad;
DESCRIBE visita_propiedades;
DESCRIBE operaciones_venta;
DESCRIBE contratos_alquiler;
DESCRIBE canales_utilizados;
DESCRIBE tipo_inspeccion;
DESCRIBE tipo_documentos_inmobiliarios;
DESCRIBE tipo_propiedad;
DESCRIBE documentos_inmobiliarios;
DESCRIBE clientes_potenciales;
DESCRIBE propiedad;
DESCRIBE propietario;
DESCRIBE inspecciones_tecnicas;
DESCRIBE agentes_inmobiliarios;
DESCRIBE marketing_propiedades;

